<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE  mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="restaurant">

<insert id="create" parameterType="String">
 	INSERT INTO restaurant(tour_seq,tour_post_sn,mem_seq,res_telnum,res_adress,res_open_time,res_desc,res_name)
 	values(1,1,1,01011223344,'here','1-12','hello',${map})
</insert>
<select id="list" parameterType="int" resultType="RestaurantVO" >
	SELECT res_name,res_adress,res_desc,res_image
	FROM restaurant
	WHERE res_code = ${index}
</select>

</mapper>